{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.0",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "nwidart/laravel-modules": "^12.0",
        "phpoffice/phpspreadsheet": "^5.4",
        "spatie/laravel-backup": "^9.0",
        "spatie/laravel-permission": "^6.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\Accounting\\": "Modules/Accounting/app/",
            "Modules\\Accounting\\Database\\Seeders\\": "Modules/Accounting/database/seeders/",
            "Modules\\CRM\\": "Modules/CRM/app/",
            "Modules\\CRM\\Database\\Seeders\\": "Modules/CRM/database/seeders/",
            "Modules\\Inventory\\": "Modules/Inventory/app/",
            "Modules\\Inventory\\Database\\Seeders\\": "Modules/Inventory/database/seeders/",
            "Modules\\SuperAdmin\\": "Modules/SuperAdmin/app/",
            "Modules\\SuperAdmin\\Database\\Seeders\\": "Modules/SuperAdmin/database/seeders/",
            "Modules\\HumanResources\\": "Modules/HumanResources/app/",
            "Modules\\HumanResources\\Database\\Seeders\\": "Modules/HumanResources/database/seeders/",
            "Modules\\Manufacturing\\": "Modules/Manufacturing/app/",
            "Modules\\Manufacturing\\Database\\Seeders\\": "Modules/Manufacturing/database/seeders/",
            "Modules\\Sales\\": "Modules/Sales/app/",
            "Modules\\Sales\\Database\\Seeders\\": "Modules/Sales/database/seeders/",
            "Modules\\FixedAssets\\": "Modules/FixedAssets/app/",
            "Modules\\FixedAssets\\Database\\Seeders\\": "Modules/FixedAssets/database/seeders/",
            "Modules\\ServiceManagement\\": "Modules/ServiceManagement/app/",
            "Modules\\ServiceManagement\\Database\\Seeders\\": "Modules/ServiceManagement/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1 --timeout=0\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform-check": false,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        },
        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org",
            "options": {
                "ssl": {
                    "verify_peer": false,
                    "verify_peer_name": false,
                    "allow_self_signed": true
                }
            }
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
